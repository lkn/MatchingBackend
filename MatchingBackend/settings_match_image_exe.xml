<!-- sorry but I only handle landscape images =] -->

<settings>
	<query path="data/demo-aesop-square-queries-320x240/images.txt">
		<!-- parameters to cvSurfParam -->
		<surf>
			<hessian_threshold>1000</hessian_threshold>
			<extended_parameter>0</extended_parameter>
		</surf>
		
		<!-- pixel sizes (if empty, use original image size) -->
		<image>
			<width>320</width>
			<height>240</height>
		</image>
		
	</query>
	
	<library path="data/demo-aesop-square/index.xml">
		<!-- parameters to cvSurfParam -->
		<surf>
			<hessian_threshold>1000</hessian_threshold>
			<extended_parameter>0</extended_parameter>
		</surf>
		
		<!-- pixel sizes -->
		<image>
			<width>300</width>
			<height>300</height>
		</image>
		
		<!-- double for percentage -->
		<match_threshold>15</match_threshold>
	</library>
	
	<!-- if empty, then logger uses default file name ({date}.txt) -->
	<log name="" />
</settings>
